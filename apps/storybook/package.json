{
  "name": "storybook",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "storybook dev -p 6006",
    "story": "stories",
    "storybook": "storybook dev -p 6006",
    "build": "storybook build",
    "preview": "npx http-server storybook-static -p 6007 -o",
    "sync": "svelte-kit sync",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "5.0.0-next.1",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/addon-svelte-csf": "^5.0.8",
    "@storybook/addon-themes": "^9.1.5",
    "@storybook/blocks": "^8.6.14",
    "@storybook/manager-api": "^8.6.14",
    "@storybook/sveltekit": "^9.1.5",
    "@sveltejs/adapter-auto": "^6.1.0",
    "@sveltejs/kit": "^2.37.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.13",
    "storybook": "^9.1.5",
    "svelte": "^5.38.8",
    "svelte-check": "^4.3.1",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "vite": "^7.1.5"
  },
  "dependencies": {
    "@layerd/config-svelte": "workspace:*",
    "@layerd/config-vite": "workspace:*",
    "@layerd/tools": "workspace:*",
    "@layerd/ui": "workspace:*"
  }
}