{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"allowSyntheticDefaultImports": true,
		"isolatedModules": true,
		"target": "ES2022",
		"lib": ["DOM", "DOM.Iterable", "ES6"]
	}
}
